    <profile id="02d8-024a-e48e-fbd7" name="Heroic Move" book="Middle-earth Strategy Battle Game Rules Manual" page="69" hidden="false" profileTypeId="62f4-755b-f936-8dd6" profileTypeName="Heroic Action">
      <profiles/>
      <rules/>
      <infoLinks/>
